{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "./dist"
  },
  "productName": "Mosaic Launcher",
  "version": "0.1.0",
  "identifier": "com.mosaic.launcher",
  "app": {
    "windows": [
      {
        "title": "Mosaic Launcher",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "resizable": true,
        "maximizable": true,
        "minimizable": true,
        "closable": true,
        "transparent": true,
        "decorations": false,
        "alwaysOnTop": false,
        "visible": true
      }
    ],
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  }
} 